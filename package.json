{
  "name": "pals-bot",
  "version": "1.0.0",
  "description": "Telegram漂流瓶机器人",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "pm2:start": "npm run build && pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop pals-bot",
    "pm2:restart": "pm2 restart pals-bot",
    "pm2:logs": "pm2 logs pals-bot"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "https-proxy-agent": "^7.0.6",
    "moment": "^2.30.1",
    "node-cron": "^3.0.3",
    "socks-proxy-agent": "^8.0.5",
    "sqlite3": "^5.1.6",
    "telegraf": "^4.15.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.0",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "telegram",
    "bot",
    "drift-bottle",
    "message-in-bottle",
    "social",
